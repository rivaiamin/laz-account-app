{
  "name": "mas-scope-app",
  "version": "0.3.9",
  "description": "MAS By Scope",
  "main": "electron.js",
  "build": {
    "appId": "com.scope.mas",
    "mac": {
      "category": "public.app-category.utilities"
    },
    "win": {
      "target": "portable",
      "icon": "build/mas.png"
    },
    "files": [
      "!data/db.json",
      "!docs/",
      "!src/",
      "!dist/",
      "!.git/",
      "!.vscode/",
      "!node_modules/@types/node",
      "!node_modules/@types/webpack-env",
      "!node_modules/autoprefixer",
      "!node_modules/css-loader",
      "!node_modules/electron",
      "!node_modules/electron-builder",
      "!node_modules/file-loader",
      "!node_modules/html-loader",
      "!node_modules/html-webpack-plugin",
      "!node_modules/html-webpack-template",
      "!node_modules/image-webpack-loader",
      "!node_modules/json-server",
      "!node_modules/mini-css-extract-plugin",
      "!node_modules/node-sass",
      "!node_modules/postcss-loader",
      "!node_modules/raw-loader",
      "!node_modules/rimraf",
      "!node_modules/sass-loader",
      "!node_modules/serve",
      "!node_modules/style-loader",
      "!node_modules/ts-loader",
      "!node_modules/typescript",
      "!node_modules/url-loader",
      "!node_modules/webpack",
      "!node_modules/.bin/",
      "!node_modules/.cache/"
    ]
  },
  "scripts": {
    "start": "electron .",
    "front-start": "webpack-dev-server --mode development --host 0.0.0.0",
    "dev": "concurrently \"webpack --watch\" \"electron .\"",
    "build": "rimraf ./public/*.js && webpack --progress -p",
    "pack": "electron-builder --dir",
    "dist": "rimraf ./dist/* && electron-builder --win --linux --ia32 --x64"
  },
  "repository": "https://github.com/rivaiamin/laz-account-app",
  "keywords": [
    "mas",
    "account",
    "Electron"
  ],
  "author": "GitHub",
  "license": "CC0-1.0",
  "devDependencies": {
    "@types/node": "^12.12.17",
    "@types/webpack-env": "^1.14.1",
    "autoprefixer": "^9.7.3",
    "css-loader": "0.26.1",
    "electron": "^13.6.6",
    "electron-builder": "^21.2.0",
    "file-loader": "^5.0.2",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "html-webpack-template": "^6.2.0",
    "image-webpack-loader": "^6.0.0",
    "json-server": "^0.15.1",
    "mini-css-extract-plugin": "^0.8.0",
    "node-sass": "^4.13.0",
    "postcss-loader": "^3.0.0",
    "raw-loader": "^4.0.0",
    "rimraf": "2.5.4",
    "sass-loader": "^8.0.0",
    "serve": "^11.2.0",
    "style-loader": "^0.13.1",
    "ts-loader": "^6.2.1",
    "typescript": "^3.7.3",
    "url-loader": "^3.0.0",
    "webpack": "^4.41.4"
  },
  "dependencies": {
    "angular": "^1.7.9",
    "angular-expressions": "^1.0.1",
    "angular-i18n": "^1.7.9",
    "angular-messages": "^1.7.9",
    "angular-resource": "^1.7.9",
    "angular-sanitize": "^1.7.9",
    "angular-ui-router": "^1.0.23",
    "body-parser": "^1.19.0",
    "busboy": "^0.3.1",
    "collect.js": "^4.18.9",
    "connect-busboy": "0.0.2",
    "cors": "^2.8.5",
    "dotenv": "^4.0.0",
    "excel4node": "^1.7.2",
    "express": "^4.17.1",
    "font-awesome": "^4.7.0",
    "getmac": "^5.15.0",
    "intl": "^1.2.5",
    "jquery": "^3.4.1",
    "lodash": "^4.17.15",
    "lowdb": "^1.0.0",
    "morgan": "^1.9.1",
    "ng-file-upload": "^12.2.13",
    "ngstorage": "^0.3.11",
    "node-xlsx": "^0.15.0",
    "pizzip": "^3.0.6",
    "popper.js": "^1.16.0",
    "semantic-ui": "^2.4.2",
    "semantic-ui-css": "^2.4.1",
    "sweetalert2": "^8.19.0"
  },
  "config": {
    "app_id": "mas"
  }
}
